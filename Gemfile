source "https://rubygems.org"

# Use activerecord as our database ORM
gem "activerecord"

# use active_record_upsert for easy updating of records
gem "active_record_upsert"

# use pandata to retrieve my pandora data
gem "pandata"

# Use pg as our database adapter
gem "pg"

# use rufus-scheduler to time when data gets loaded
gem "rufus-scheduler"
